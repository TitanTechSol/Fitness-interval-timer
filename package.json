{
  "name": "fitness-interval-timer",
  "version": "1.2.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . fitness-interval-timer --platform=win32 --arch=x64 --out=dist --overwrite --ignore=node_modules/electron-builder",
    "build-all": "electron-packager . fitness-interval-timer --all --out=dist --overwrite",
    "dist": "npm run build",
    "clean": "rimraf dist node_modules",
    "setup": "npm install && npm run dist",
    "quick-start": "npm install && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fitness",
    "timer",
    "interval",
    "workout",
    "electron"
  ],
  "author": "George Aaron Kibbie",
  "license": "MIT",
  "description": "A customizable fitness interval timer with audio cues and multiple themes",
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-packager": "^17.1.2"
  }
}
